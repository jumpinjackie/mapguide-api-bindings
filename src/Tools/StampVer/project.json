{
    "version": "0.0.1",
    "description": "Version Stamping Tool for mapguide-api-bindings",
    "authors": [ "Open Source Geospatial Foundation" ],
    "tags": [ "MapGuide" ],
    "projectUrl": "http://mapguide.osgeo.org",
    "licenseUrl": "http://www.gnu.org/licenses/lgpl-2.1.txt",
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [ "DEBUG", "TRACE" ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [ "RELEASE", "TRACE" ],
                "optimize": true
            }
        }
    },
    "frameworks": {
        "dnx451": { },
        "dnxcore50": {
            "dependencies": {
                "Microsoft.CSharp": "4.0.1-beta-23409",
                "System.Console": "4.0.0-beta-23516",
                "System.Collections": "4.0.11-beta-23409",
                "System.Linq": "4.0.1-beta-23409",
                "System.Runtime": "4.0.21-beta-23409",
                "System.IO.FileSystem": "4.0.1-beta-23516"
            }
        }
    },
    "sources": {
        "compile": [ "*.cs" ]
    },
    "dependencies": {
        "Newtonsoft.Json": "8.0.2",
        "System.Text.RegularExpressions": "4.0.11-beta-23516"
    }
}